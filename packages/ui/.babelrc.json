{
    "plugins": [
        ["@babel/plugin-proposal-decorators", { "decoratorsBeforeExport": true }],
        ["@babel/plugin-proposal-class-properties", { "loose": true }]
    ],
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "chrome": 100
                }
            }
        ],
        "@babel/preset-typescript",
        "@babel/preset-react"
    ],
    "sourceType": "unambiguous"
}
