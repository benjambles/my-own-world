        "http-errors": "^1.7.3",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.17.6",
        "koa": "^2.7.0",
        "koa-404-handler": "0.0.2",
        "koa-better-error-handler": "^2.0.0",
        "koa-compress": "^3.0.0",
        "koa-conditional-get": "^2.0.0",
        "koa-etag": "^3.0.0",
        "koa-helmet": "^4.2.0",
        "koa-joi-router": "^6.0.0",
        "koa-json-error": "^3.1.2",
        "koa-jwt": "^3.5.1",
        "koa-morgan": "^1.0.1",
        "koa-response-time": "^2.1.0",
        "koa-router": "^7.4.0",
        "nodemailer": "^5.1.1",
        "pg": "^7.11.0",
        "pg-extra": "2.0.1",
        "ramda": "^0.26.1",
        "ramda-adjunct": "^2.18.0",
        "rimraf": "^2.6.3",
        "semver": "^6.1.2",
        "semver-compare": "^1.0.0",
        "uuid": "^3.3.2"
