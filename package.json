{
    "name": "my-own-world",
    "version": "1.0.0",
    "description": "Tools for world builders",
    "main": "index.js",
    "scripts": {
        "api": "nodemon  --watch dist/api ./dist/api/bin/api.js",
        "api:build": "tsc --project ./src/api && gulp --cwd ./src/api",
        "api:watch:ts": "tsc --project ./src/api --watch",
        "api:test": "jest --projects ./src/api",
        "api:test:update": "jest --projects ./src/api -u",
        "api:test:update:changed": "jest --projects ./src/api -u --changedSince=master",
        "api:test:coverage": "jest --projects ./src/api --coverage",
        "api:test:coverage:changed": "jest --projects ./src/api --coverage --changedSince=master",
        "api:test:clearCache": "jest --clearCache",
        "api:generator": "node ./dist/api/lib/generators/index.js",
        "website": "nodemon --watch dist/website ./dist/website/bin/app.js",
        "website:build": "tsc --project ./src/website && gulp --cwd ./src/website",
        "website:watch:ts": "tsc --project ./src/website --watch",
        "website:watch:css": "gulp --cwd ./src/website watchCSS",
        "storybook": "start-storybook"
    },
    "author": "Ben Allen",
    "license": "ISC",
    "dependencies": {
        "@popeindustries/lit-html-server": "3.0.0",
        "bcrypt": "4.0.1",
        "classnames": "^2.2.6",
        "commander": "5.0.0",
        "dotenv": "8.2.0",
        "fp-ts": "^1.19.3",
        "http-errors": "1.7.3",
        "jsonwebtoken": "8.5.1",
        "koa": "2.11.0",
        "koa-404-handler": "0.0.2",
        "koa-better-error-handler": "3.0.5",
        "koa-compress": "3.0.0",
        "koa-conditional-get": "2.0.0",
        "koa-etag": "3.0.0",
        "koa-helmet": "5.2.0",
        "koa-joi-router": "6.0.2",
        "koa-json-error": "3.1.2",
        "koa-jwt": "3.6.0",
        "koa-morgan": "1.0.1",
        "koa-response-time": "2.1.0",
        "koa-router": "8.0.8",
        "koa-static": "^5.0.0",
        "lit-element": "2.3.1",
        "lit-html": "1.2.1",
        "mongodb": "3.5.5",
        "nodemailer": "6.4.6",
        "ramda": "0.27.0",
        "ramda-adjunct": "2.26.0",
        "rimraf": "3.0.2",
        "semver": "7.1.3",
        "semver-compare": "1.0.0",
        "uuid": "7.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@storybook/addon-actions": "^5.3.18",
        "@storybook/addon-knobs": "^5.3.18",
        "@storybook/addon-notes": "^5.3.18",
        "@storybook/addons": "^5.3.18",
        "@storybook/html": "^5.3.18",
        "@types/bcrypt": "3.0.0",
        "@types/classnames": "^2.2.10",
        "@types/http-errors": "1.6.3",
        "@types/jest": "25.1.4",
        "@types/joi": "14.3.4",
        "@types/jsonwebtoken": "8.3.8",
        "@types/koa": "2.11.3",
        "@types/koa-morgan": "1.0.4",
        "@types/koa-router": "7.4.0",
        "@types/node": "13.9.5",
        "@types/nodemailer": "6.4.0",
        "@types/ramda": "0.27.0",
        "@types/semver": "7.1.0",
        "@types/uuid": "7.0.2",
        "@typescript-eslint/eslint-plugin": "2.25.0",
        "@typescript-eslint/parser": "2.25.0",
        "autoprefixer": "^9.7.5",
        "babel-loader": "^8.1.0",
