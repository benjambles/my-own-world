{
    "name": "my-own-world",
    "version": "1.0.0",
    "description": "Tools for world builders",
    "main": "index.js",
    "scripts": {
        "test:server": "jest --projects ./src/server",
        "test:server:update": "jest --projects ./src/server -u",
        "test:server:update:changed": "jest --projects ./src/server -u --changedSince=master",
        "test:server:coverage": "jest --projects ./src/server --coverage",
        "test:server:coverage:changed": "jest --projects ./src/server --coverage --changedSince=master",
        "test:server:clearCache": "jest --clearCache",
        "build:server": "gulp --cwd ./src/server",
        "run:server": "nodemon ./built/server/bin/api.js",
        "generator": "node ./built/server/lib/generators/index.js"
    },
    "author": "Ben Allen",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^3.0.6",
        "commander": "^2.20.0",
        "dotenv": "^6.2.0",
        "http-errors": "^1.7.2",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.16.5",
        "koa": "^2.7.0",
        "koa-404-handler": "0.0.2",
        "koa-better-error-handler": "^2.0.0",
        "koa-compress": "^3.0.0",
        "koa-conditional-get": "^2.0.0",
        "koa-etag": "^3.0.0",
        "koa-helmet": "^4.1.0",
        "koa-joi-router": "^5.3.0",
        "koa-json-error": "^3.1.2",
        "koa-jwt": "^3.5.1",
        "koa-morgan": "^1.0.1",
        "koa-response-time": "^2.1.0",
        "koa-router": "^7.4.0",
        "nodemailer": "^5.1.1",
        "pg": "^7.10.0",
        "pg-extra": "2.0.1",
        "ramda": "^0.26.1",
        "rimraf": "^2.6.3",
        "semver": "^6.0.0",
        "semver-compare": "^1.0.0",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/http-errors": "^1.6.1",
        "@types/jest": "^24.0.6",
        "@types/joi": "^14.3.3",
        "@types/jsonwebtoken": "^8.3.2",
        "@types/knex": "0.16.1",
        "@types/koa": "^2.0.48",
        "@types/koa-jwt": "^3.3.0",
        "@types/koa-morgan": "^1.0.4",
