{
    "name": "my-own-world",
    "version": "1.0.0",
    "description": "Tools for world builders",
    "main": "index.js",
    "browserslist": [
        "> 1% and last 2 versions",
        "not IE 11"
    ],
    "scripts": {
        "build": "tsc -b -v && gulp",
        "test": "jest",
        "clean:ts": "tsc -b --clean",
        "watch:ts": "tsc -b -v -w",
        "watch:css": "gulp --cwd src/ui watchCSS",
        "api": "nodemon --config src/api/nodemon.json --inspect=:9230 dist/api/bin/app.js",
        "website": "nodemon --config src/website/nodemon.json --inspect=:9229 dist/website/bin/app.js",
        "storybook": "start-storybook -c src/ui/.storybook -s dist/ui/static",
        "api:generator": "node src/api/lib/generators/index.js"
    },
    "author": "Ben Allen",
    "license": "ISC",
    "dependencies": {
        "@popeindustries/lit-html-server": "3.1.0",
        "bcrypt": "5.0.0",
        "dotenv": "8.2.0",
        "http-errors": "1.8.0",
        "jsonwebtoken": "8.5.1",
        "koa": "2.13.0",
        "koa-better-error-handler": "6.0.1",
        "koa-compress": "5.0.1",
        "koa-conditional-get": "3.0.0",
        "koa-etag": "4.0.0",
        "koa-helmet": "6.0.0",
        "koa-joi-router": "7.0.0",
        "koa-jwt": "4.0.0",
        "koa-pino-logger": "3.0.0",
        "koa-response-time": "2.1.0",
        "koa-router": "10.0.0",
        "koa-static": "5.0.0",
        "lit-element": "2.4.0",
        "lit-html": "1.3.0",
        "mongodb": "3.6.3",
        "ramda": "0.27.1",
        "ts-option": "2.1.0",
        "uuid": "8.3.2"
    },
    "devDependencies": {
        "@babel/core": "7.12.10",
        "@shopify/jest-koa-mocks": "2.3.0",
        "@storybook/addon-a11y": "6.1.11",
        "@storybook/addon-essentials": "6.1.11",
        "@storybook/addon-knobs": "6.1.11",
        "@storybook/addons": "6.1.11",
        "@storybook/html": "6.1.11",
        "@types/bcrypt": "3.0.0",
        "@types/http-errors": "1.8.0",
        "@types/jest": "26.0.19",
        "@types/jsonwebtoken": "8.5.0",
        "@types/koa": "2.11.6",
        "@types/koa-joi-router": "5.2.4",
        "@types/node": "14.14.16",
        "@types/ramda": "0.27.34",
        "@types/uuid": "8.3.0",
        "@typescript-eslint/eslint-plugin": "4.11.0",
        "@typescript-eslint/parser": "4.11.0",
        "css-loader": "5.0.1",
        "cssnano": "4.1.10",
        "eslint": "7.16.0",
        "gulp": "4.0.2",
        "gulp-postcss": "9.0.0",
        "husky": "4.3.6",
        "jest": "26.6.3",
        "lint-staged": "10.5.3",
        "postcss": "8.2.1",
        "postcss-cli": "8.3.1",
        "postcss-import": "14.0.0",
        "postcss-loader": "4.1.0",
        "postcss-modules": "4.0.0",
        "postcss-nesting": "7.0.1",
        "prettier": "2.2.1",
        "style-loader": "2.0.0",
