{
    "name": "my-own-world",
    "version": "1.0.0",
    "description": "Tools for world builders",
    "main": "index.js",
    "scripts": {
        "test:server": "jest --projects ./src/api",
        "test:api:update": "jest --projects ./src/api -u",
        "test:api:update:changed": "jest --projects ./src/api -u --changedSince=master",
        "test:api:coverage": "jest --projects ./src/api --coverage",
        "test:api:coverage:changed": "jest --projects ./src/api --coverage --changedSince=master",
        "test:api:clearCache": "jest --clearCache",
        "build:api": "gulp --cwd ./src/api",
        "run:api": "nodemon ./built/api/bin/api.js ",
        "generator": "node ./built/api/lib/generators/index.js"
    },
    "author": "Ben Allen",
    "license": "ISC",
